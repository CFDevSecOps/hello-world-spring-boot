apiVersion: apps/v1
kind: Deployment
metadata:
  name: javademo1
spec:
  selector:
    matchLabels:
      app: javademo1
  replicas: 2
  template:
    metadata:
      labels:
        app: javademo1
    spec:
      containers:
      - name: javademo1
        image: argocd.jfrog.io/java-docker/java-app:latest
        ports:
        - containerPort: 8080
